<!DOCTYPE html>
<html>
  <%- include('layout/head'); -%>
  <body>
    <%- include('partials/searchbox'); -%>
    <h2>Results in <%= l1 %> for "<%= query %>"</h2>
    <%- include('partials/terms'); -%>
    <ul>
      <% for(const phrase of phrases) { %>
      <li>
        <div><b><%= l1 %></b>: <%- phrase.de %></div>
        <div><b><%= l2 %></b>: <%- phrase.nl %></div>
      </li>
      <% } %>
    </ul>
  </body>
</html>
